{
  "name": "@elastic/next-eui-starter",
  "private": true,
  "description": "Start building Kibana protoypes quickly with the Next.js EUI Starter",
  "version": "1.0.0",
  "author": "Rory Hunter <rory@elastic.co>",
  "keywords": [
    "next",
    "kibana",
    "eui",
    "elastic",
    "typescript"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "yarn lint && rm -f public/themes/*.min.css && next build",
    "build-docs": "bash scripts/update-docs.sh",
    "dev": "next",
    "lint": "eslint --cache 'src/**/*.{ts,tsx,js}' --max-warnings 0",
    "start": "next start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elastic/next-eui-starter"
  },
  "bugs": {
    "url": "https://github.com/elastic/next-eui-starter/issues"
  },
  "dependencies": {
    "@elastic/datemath": "^5.0.2",
    "@elastic/eui": "18.2.1",
    "@next/bundle-analyzer": "^9.3.1",
    "glob": "^7.1.6",
    "moment": "^2.24.0",
    "next": "^9.3.2",
    "next-images": "^1.3.1",
    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/node": "^13.9.3",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "@zeit/next-sass": "^1.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "null-loader": "^3.0.0",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  }
}
