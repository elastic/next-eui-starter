{
  "name": "@elastic/next-eui-starter",
  "private": true,
  "description": "Start building protoypes quickly with the Next.js EUI Starter",
  "version": "1.0.0",
  "author": "Rory Hunter <rory@elastic.co>",
  "keywords": [
    "next",
    "kibana",
    "eui",
    "elastic",
    "typescript"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "yarn lint && rm -f public/themes/*.min.css && next build",
    "build-docs": "bash scripts/update-docs.sh",
    "dev": "next",
    "lint": "tsc && eslint --cache 'src/**/*.{ts,tsx,js}' --max-warnings 0",
    "start": "next start",
    "test-docs": "bash scripts/test-docs.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elastic/next-eui-starter"
  },
  "bugs": {
    "url": "https://github.com/elastic/next-eui-starter/issues"
  },
  "dependencies": {
    "@elastic/datemath": "^5.0.3",
    "@elastic/eui": "^30.1.1",
    "@next/bundle-analyzer": "^10.0.0",
    "glob": "^7.1.6",
    "moment": "^2.29.1",
    "next": "^10.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.28.0",
    "typescript-plugin-css-modules": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "copy-webpack-plugin": "^6.2.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "iniparser": "^1.0.5",
    "null-loader": "^4.0.1",
    "prettier": "^2.1.2",
    "serve": "^11.3.2",
    "typescript": "^4.0.5"
  }
}
